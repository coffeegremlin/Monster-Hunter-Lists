  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>
      <a href="/" style="color: #bd5624;">
        <i type="submit" class="fas fa-home"></i>
      </a>
      <a href="/profiles/<%= user.profile._id %>/weapon" style="color: #bd5624;">
        <i type="submit" class="fas fa-scroll"></i>
      </a>
      <form id="nav-search" action="/weaponSearch" method="POST">
        <input name="query" type="text" placeholder="Search Weapons">
          <button type="submit" class="fas fa-search" style="color: #bd5624;"></button>
        </form>
        <a href="/auth/logout" style="color: #bd5624;">
          <i class="fas fa-sign-out-alt"></i>
        </a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
